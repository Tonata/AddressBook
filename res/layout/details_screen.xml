<?xml version="1.0" encoding="utf-8"?>

<AbsoluteLayout xmlns:android="http://schemas.android.com/apk/res/android"
                android:layout_width="match_parent"
                android:layout_height="match_parent">

    <TextView
            android:layout_width="73dp"
            android:layout_height="34dp"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="Email"
            android:id="@+id/viewEmail" android:layout_x="147dp" android:layout_y="221dp"/>
    <TextView
            android:layout_width="130dp"
            android:layout_height="36dp"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="Cell Number"
            android:id="@+id/viewCell" android:layout_x="125dp" android:layout_y="309dp"/>
    <TextView
            android:layout_width="142dp"
            android:layout_height="42dp"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="Home Address"
            android:id="@+id/viewHome" android:layout_x="118dp" android:layout_y="384dp"/>
    <Button
            style="?android:attr/buttonStyleSmall"
            android:layout_width="93dp"
            android:layout_height="49dp"
            android:text="Back"
            android:id="@+id/btnBack" android:layout_x="19dp" android:layout_y="488dp"/>
    <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="CONTACT DETAILS"
            android:id="@+id/textView" android:layout_x="83dp" android:layout_y="21dp"/>
    <EditText
            android:layout_width="249dp"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/viewName" android:layout_x="64dp" android:layout_y="88dp" android:enabled="false"
            />
    <EditText
            android:layout_width="245dp"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/viewLast" android:layout_x="62dp" android:layout_y="172dp" android:enabled="false"/>
    <EditText
            android:layout_width="240dp"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/tEmail" android:layout_x="64dp" android:layout_y="253dp" android:enabled="false"/>
    <EditText
            android:layout_width="247dp"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/tCell" android:layout_x="61dp" android:layout_y="332dp" android:enabled="false"/>
    <EditText
            android:layout_width="240dp"
            android:layout_height="wrap_content"
            android:inputType="textMultiLine"
            android:ems="10"
            android:id="@+id/tHome" android:layout_x="61dp" android:layout_y="413dp" android:enabled="false"/>
    <TextView
            android:layout_width="132dp"
            android:layout_height="32dp"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="First Name"
            android:id="@+id/textView2" android:layout_x="123dp" android:layout_y="55dp"/>
    <TextView
            android:layout_width="122dp"
            android:layout_height="35dp"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="Last Name"
            android:id="@+id/textView3" android:layout_x="125dp" android:layout_y="144dp"/>
</AbsoluteLayout>